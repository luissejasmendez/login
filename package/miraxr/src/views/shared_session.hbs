<section class="logo-container">
    <img src="{{{org.avatar}}}" alt="{{{org.name}}}" height="30" />
</section>
<section class="input-container" id="shared-session-login-partial">
    <section class="text-container">
        <p>
            {{{__ "shared_session.welcome_message"}}}
        </p>
    </section>
    <label> {{{__ "shared_session.shared_session_form.key_label"}}}</label>
    <i class="fa fa-key"></i>
    <input type="text" class="email-input shared-session-key-input" placeholder="{{{__ "shared_session.shared_session_form.key_placeholder"}}}" id="shared-session-key" autocomplete="off" autofocus/>
</section>
<section class="buttons-container">
    <button class="primary disabled" id="shared-session-login-btn">
        {{{__ "shared_session.shared_session_form.next_btn"}}}
    </button>
    <button class="primary" id="back-btn-sharedSessionScreen" style="display: none;">
        {{{__ "shared_session.shared_session_form.back_btn"}}}
    </button>
    <p class="err-container"></p>
</section>
<section class="link-container">
    <a href="javascript: void(0)" id="navigate-to-login-link">{{{__ "login.login_form.navigate_to_login_label"}}}</a>
</section>
